{
  "name": "trusted-resources",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Matthew Keil",
  "description": "",
  "scripts": {
    "start": "run-p start:libs start:uis",
    "build": "npm run build:libs && npm run build:core && npm run build:stack",
    "clean": "npm run clean --workspaces",
    "clean:nm": "npm run clean:nm --workspaces",
    "post:clean:nm": "rm -rf node_modules",
    "build:contracts": "npm run build --workspace=@trusted-resources/contracts",
    "build:front": "npm run build --workspace=@trusted-resources/frontend",
    "start:contracts": "npm run start --workspace=@trusted-resources/contracts",
    "start:front": "npm start --workspace=@trusted-resources/frontend",
    "start:uis": "run-p start:ui:*",
    "synth": "npm run synth --workspace=@trusted-resources/infra",
    "deploy": "npm run deploy --workspace=infra",
    "destroy": "npm run destroy --workspace=infra",
    "node": "node",
    "ts": "npm run node -- -r ts-node/register"
  },
  "workspaces": [
    "packages/contracts",
    "packages/frontend",
    "packages/infra"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "private": true,
  "devDependencies": {
    "esbuild": "^0.15.10",
    "npm-run-all": "^4.1.5"
  }
}
